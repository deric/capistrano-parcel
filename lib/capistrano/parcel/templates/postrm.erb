#!/bin/sh

set -e

APP_NAME=<%= fetch(:application) %>

case "$1" in
    remove)
      <% deb_postrm.each do |line| -%>
      <%= line %>
      <% end -%>
	;;

    purge)
	;;

    upgrade)
	;;

    failed-upgrade|disappear|abort-install|abort-upgrade)
	;;
    *)
	echo "$0 called with unknown argument \`$1'" 1>&2
	exit 1
	;;
esac

exit 0
